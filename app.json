{
  "expo": {
    "name": "naqaa-cars-management",
    "slug": "naqaa-cars-management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "naqaacarsmanagement",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "navigationBar": {
        "visible": "sticky-immersive",
        "behavior": "overlay-swipe",
        "backgroundColor": "#00000001"
      },
      "softwareKeyboardLayoutMode": "pan",
      "fullScreen": true,
      "package": "com.mohamedkhaled68.naqaacarsmanagement"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3c634be5-cdd0-4c96-825c-fb7b871567b8"
      }
    }
  }
}

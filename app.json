{
    "expo": {
        "name": "Naqaa",
        "slug": "naqaa-cars-management",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/bb.png",
        "scheme": "naqaacarsmanagement",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.mohamedkhaled68.naqaacarsmanagement",
            "buildNumber": "1",
            "infoPlist": {
                "NSCameraUsageDescription": "This app uses the camera to capture vehicle meter readings for maintenance tracking.",
                "NSPhotoLibraryUsageDescription": "This app accesses the photo library to select images of vehicle meter readings.",
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/bb.png",
                "backgroundColor": "#000"
            },
            "package": "com.mohamedkhaled68.naqaacarsmanagement"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-font",
            "expo-web-browser"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "3c634be5-cdd0-4c96-825c-fb7b871567b8"
            }
        }
    }
}
